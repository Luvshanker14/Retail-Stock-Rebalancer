{
  "dashboard": {
    "id": null,
    "title": "Phase 1 Analytics Dashboard",
    "tags": ["retail", "analytics", "phase1"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "üìä Sales Performance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(purchases_total)",
            "legendFormat": "Total Purchases"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "üí∞ Total Revenue",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(revenue_total)",
            "legendFormat": "Total Revenue (‚Çπ)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "unit": "currencyINR"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "üìà Daily Sales Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "daily_sales",
            "legendFormat": "{{store_name}} - {{date}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "üí∞ Daily Revenue Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "daily_revenue",
            "legendFormat": "{{store_name}} - {{date}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "unit": "currencyINR"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "üè™ Store Performance Comparison",
        "type": "barchart",
        "targets": [
          {
            "expr": "store_total_revenue",
            "legendFormat": "{{store_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "unit": "currencyINR"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "üì¶ Stock Levels by Store",
        "type": "barchart",
        "targets": [
          {
            "expr": "sum by (store_name) (stock_level)",
            "legendFormat": "{{store_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "‚ö†Ô∏è Low Stock Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(low_stock_products)",
            "legendFormat": "Products with Low Stock"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "‚ùå Out of Stock Products",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(out_of_stock_products)",
            "legendFormat": "Out of Stock Products"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "üèÜ Store Performance Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "store_performance_score",
            "legendFormat": "{{store_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 80
                }
              ]
            },
            "max": 100,
            "min": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "üî• Top Selling Products",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, product_sales_count)",
            "legendFormat": "{{product_name}} - {{store_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "üíé Top Revenue Products",
        "type": "table",
