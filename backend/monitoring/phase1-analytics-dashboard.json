          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "store_id",
          "type": "query",
          "query": "label_values(stores_added_total, store_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
